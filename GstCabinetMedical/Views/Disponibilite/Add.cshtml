@model Disponibilite
<div class="main-panel">
    <div class="content-wrapper">
        <div class="page-header">
            <nav aria-label="breadcrumb">
            </nav>
        </div>
        <div class="row">
            <div class="col-md-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <form class="forms-sample" method="post">
                            <div asp-validation-summary="All" />
                            <input type="hidden" asp-for="MedcinID" />
                            <span asp-validation-for="MedcinID"></span>
                            <!-- Champ caché pour l'ID du médecin -->
                            <div class="form-group">
                                <label for="date">Date</label>
                                <input type="date"class="form-control" asp-for="DateRendezvous" placeholder="Date"/>
                                <span asp-validation-for="DateRendezvous"></span>
                            </div>
                            <div class="form-group">
                                <label for="Time">Horraire</label>
                                <input type="time" class="form-control" asp-for="Horraire" placeholder="Horraire"/>
                                <span asp-validation-for="Horraire"></span>
                            </div>
                            <button type="submit" class="btn btn-success btn-rounded btn-fw">Ajouter</button>
                           
                        </form>
                    </div>
                </div>
            </div>
            </div>
            </div>
            </div>
